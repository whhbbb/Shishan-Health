{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^Xs(.*)": "@/components/Xs$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/my/my",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/ranking/ranking",
      "style": {
        "navigationBarTitleText": "排行榜"
      }
    },
    {
      "path": "pages/clock-in/clockIn",
      "style": {
        "navigationBarTitleText": "打卡"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#2c2c2c",
    "selectedColor": "#c0d294",
    "backgroundColor": "#fff",
    "borderStyle": "white",
    "list": [
      {
        "text": "首页",
        "pagePath": "pages/index/index",
        "iconPath": "static/image/bottomBar/index.png",
        "selectedIconPath": "static/image/bottomBar/index-active.png"
      },
      {
        "text": "我的",
        "pagePath": "pages/my/my",
        "iconPath": "static/image/bottomBar/my.png",
        "selectedIconPath": "static/image/bottomBar/my-active.png"
      }
    ]
  },
  "lazyCodeLoading": "requiredComponents"
}